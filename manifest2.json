{
  "manifest_version":2,
  "name": "AdBlocker for YouTube", 
  "description": "Blocking Ads on YouTube", 
  "version": "1.1.0", 
  "browser_action":{ 
    "default_title": "AdBlocker" },
  "permissions": [ 
    "tabs", 
    "browser",   
    "webNavigation",
    "https://www.youtube.com/" 
  ],
  "background": { 
    "scripts": [ 
      "extension.js" 
    ] 
  } 
}
